  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "typings": "dist/index.d.ts",
  "dependencies": {
    "katex": "^0.13.0",
    "react": "^17.0.1"
  },
  "devDependencies": {
    "@umijs/test": "^3.0.5",
    "dumi": "^1.1.0",
    "dumi-theme-default": "^1.0.8",
    "father-build": "^1.19.1",
    "gh-pages": "^3.0.0",
    "yorkie": "^2.0.0"
  }
}
